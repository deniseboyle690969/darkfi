contract input_spend
    param x
    private x2
    private x3
    public input
start
    set x2 x
    mul x2 x

    set x3 x2
    mul x3 x
constraint
    lc0_add x
    lc1_add x
    lc2_add x2
    enforce

    lc0_add x2
    lc1_add x
    lc2_add x3
    enforce

    lc0_add input
    lc1_add 1
    lc2_add x3
    enforce
end

